@model IEnumerable<practica1parciallppa1.Data.Entities.Afiliado>

@{
    ViewBag.Title = "Afiliados de la obra social";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Crear Nuevo Afiliado", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <p>Nombre y Apellido</p>
        </th>
        @* <th> *@
        @*     @Html.DisplayNameFor(model => model.Apellido) *@
        @* </th> *@
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        @* <th> *@
        @*     @Html.DisplayNameFor(model => model.Telefono) *@
        @* </th> *@
        <th>
            @Html.DisplayNameFor(model => model.Cuit)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName )
        </td>
        @* <td> *@
        @*     @Html.DisplayFor(modelItem => item.Apellido) *@
        @* </td> *@
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        @* <td> *@
        @*     @Html.DisplayFor(modelItem => item.Telefono) *@
        @* </td> *@
        <td>
            @Html.DisplayFor(modelItem => item.Cuit)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @* @Html.ActionLink("Details", "Details", new { id=item.Id }) | *@
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
